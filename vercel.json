{
  "version": 2,
  "name": "drone-war-game",
  "builds": [
    {
      "src": "simple_server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/simple_server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "simple_server.js": {
      "maxDuration": 10
    }
  }
}